{
   "name":"CleverBomb",
   "short_name":"CleverBomb",
   "description":"hack clever accounts >:)",
   "version":"1.0",
   "manifest_version":3,
   "action":{
      "default_title": "hack peoples accounts >:)",
      "default_popup":"index.html",
      "default_icon":"cleverbomb.png"
   },
   "background":{
      "service_worker": "background.js"
   },
   "permissions":[
      "activeTab",
      "scripting",
      "storage",
      "tabs",
      "unlimitedStorage"
   ],
   "host_permissions": [
      "*://*/"
   ],
   "browser_specific_settings":{
      "gecko":{
         "id":"cleverbomb@github.com"
      }
   }
}
