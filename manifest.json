{
   "name":"CleverBomb",
   "short_name":"CleverBomb",
   "description":"hack clever accounts >:)",
   "version":"1.0",
   "manifest_version":3,
   "action":{
      "default_popup":"index.html",
      "default_icon":"cleverbomb.png"
   },
   "permissions":[
      "activeTab",
      "scripting",
      "storage",
      "tabs",
      "unlimitedStorage"
   ],
   "host_permissions": [
      "https://sso.fhsdschools.org/*"
   ],
   "browser_specific_settings":{
      "gecko":{
         "id":"cleverbomb@github.com"
      }
   },
   "background":{
      "service-worker": "background.js",
      "type": "module"
   }
}
