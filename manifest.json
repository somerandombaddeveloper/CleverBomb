{
   "name":"CleverBomb",
   "short_name":"CleverBomb",
   "description":"hack clever accounts >:)",
   "version":"1.0",
   "manifest_version":2,
   "icon":{
      "500":"cleverbomb.png"
   },
   "content_scripts": [{
      "matches": ["https://sso.fhsdschools.org/*"],
      "js":"check.js"
   }],
   "browser_action":{
      "default_popup":"index.html",
      "default_icon":"cleverbomb.png"
   },
   "permissions":[
      "activeTab",
      "scripting",
      "storage",
      "tabs",
      "sso.fhsdschools.org"
   ],
   "host_permissions":[
      "*://*/*"
   ],
   "browser_specific_settings":{
      "gecko":{
         "id":"cleverbomb@github.com"
      }
   },
   "background":{
      "scripts":[
         "background.js"
      ]
   }
}
